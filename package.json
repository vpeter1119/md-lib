{
  "name": "@petervertesi/md-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.3.1",
  "description": "An open-source module for compiling libraries from markdown input.",
  "main": "src/index.js",
  "bin": {
    "mdlib": "bin/bin.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "docs": "node src/jsd2md.js",
    "lint:test": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky install",
    "publish": "np",
    "test": "jest",
    "version": "node src/jsd2md.js && git add docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpeter1119/md-lib.git"
  },
  "keywords": [
    "markdown"
  ],
  "author": "Peter Vertesi <info@petervertesi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vpeter1119/md-lib/issues"
  },
  "homepage": "https://github.com/vpeter1119/md-lib#readme",
  "dependencies": {
    "minimist": "^1.2.5",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "eslint": "^7.14.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jsdoc-to-markdown": "^7.0.0",
    "np": "^7.0.0",
    "prettier": "^2.2.1"
  }
}
